#include "gpio.ceu"
#include "wclock.ceu"

output high/low PIN_11;
output high/low PIN_12;
output high/low PIN_13;

code/await Blink (var int pin, var int ms) -> NEVER do
    loop do
        emit PIN_OUT(pin,high);
        await (ms)ms;
        emit PIN_OUT(pin,low);
        await (ms)ms;
    end
end

spawn Blink(11, 1000);
spawn Blink(12,  500);
spawn Blink(13,  250);

await FOREVER;
