#include "gpio.ceu"
#include "wclock.ceu"

output high/low PIN_11;
output high/low PIN_12;
output high/low PIN_13;

loop do
    emit PIN_11(low);
    emit PIN_12(low);
    emit PIN_13(low);
    await 1s;

    emit PIN_11(high);
    await 1s;
    emit PIN_11(low);

    emit PIN_12(high);
    await 500ms;
    emit PIN_12(low);

    emit PIN_13(high);
    await 250ms;
    emit PIN_13(low);
    await 1s;

    par/and do
        emit PIN_11(high);
        await 1s;
        emit PIN_11(low);
        await 1s;
    with
        emit PIN_12(high);
        await 500ms;
        emit PIN_12(low);
        await 500ms;
    with
        emit PIN_13(high);
        await 250ms;
        emit PIN_13(low);
        await 250ms;
    end
end
